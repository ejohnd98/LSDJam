[sub_resource type="GDScript" id="GDScript_omhwq"]
script/source = "extends ColorRect

@export var default_size : float = 0.005
var use_distortion = true
var progress = 1.0

func set_distortion_amount(alpha : float):
	material.set_shader_parameter(\"size\", alpha * default_size)

func _process(delta):
	if not is_visible_in_tree():
		return
	if use_distortion and progress < 1.0:
		progress += delta * 4.0
		set_distortion_amount(progress)
	elif not use_distortion and progress > 0.0:
		progress -= delta * 4.0
		set_distortion_amount(progress)
"

